{
  "name": "ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "theme-typings": "chakra-cli tokens ./src/theme.ts",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,scss,md,mdx}\"",
    "lint": "tsc --pretty --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf dist",
    "prepare": "npm run theme-typings",
    "release": "HUSKY=0 standard-version"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.5",
    "@chakra-ui/theme-tools": "^1.3.6",
		"@emotion/react": "^11.8.1",
		"@emotion/styled": "^11.8.1",
		"@theme-ui/core": "^0.13.1",
		"framer-motion": "^6.2.7",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
    "polished": "^4.1.4"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.8.2",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"ecco-configs": "*",
		"ecco-tsconfigs": "*",
    "tsup": "^5.11.13",
    "typescript": "^4.5.3"
  }
}
